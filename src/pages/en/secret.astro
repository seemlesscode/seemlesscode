---
import StylizedVideo from '@/components/StylizedVideo';
import PageLayout from '@/layouts/PageLayout.astro';
import type { PageDataType } from '@/types/page';
import { secret } from './data/secret.astro';

const { title, description, lang, alternates } = secret.data as PageDataType;
---

<PageLayout {title} {description} {lang} {alternates}>
  <StylizedVideo client:load />
</PageLayout>
